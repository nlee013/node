//prototype 객체 만들기 - 예전에 ajax 할 때 했었음

function Person(name, age) {//기본 생성자와 같은 개념
	//초기화 시켜줄 애들//생성자
	this.name = name;
	this.age = age;
}

Person.prototype.walk = function(speed) {
	
	if(speed > 30){
		
		console.log(speed + "km 속도로 뛰어갑니다.");
		return
	}
	
	console.log(speed + "km 속도로 걸어갑니다.");
}
