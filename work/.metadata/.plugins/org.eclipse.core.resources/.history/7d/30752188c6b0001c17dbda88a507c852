/*전역객체와 method

console 객체 안에 method가 있다 -종류 : dir, time, timeEnd
process 객체 -종류 : drgv, env,exit
exports 객체 
 */

var result = 0;

console.time("계산시간");//사용자 정의 aaa
console.timeEnd("계산시간");//위와 맞춰줘야된다 aaa

for(var i = 1; i <= 100; i++){
	result += i;
}

console.time("계산시간");
console.log("1부터 100까지의 합 %d", result);
